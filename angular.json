{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nni-velocity-app-onboarding": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:module": {
          "flat": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": ["src/assets", "src/favicon.ico", "src/api"],
            "styles": ["src/styles.scss"],
            "scripts": ["src/assets/js/perfect-scrollbar.min.js", "src/assets/js/es6-promise-auto.min.js"],
            "allowedCommonJsDependencies": ["papaparse", "file-saver"]
          },
          "configurations": {
            "dev": {
              "optimization": true,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "vendorChunk": false
            },
            "qa": {
              "optimization": true,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qa.ts"
                }
              ]
            },
            "train": {
              "optimization": true,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.train.ts"
                }
              ]
            },
            "prod": {
              "sourceMap": false,
              "optimization": true,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "es5": {
              "tsConfig": "tsconfig-es5.app.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nni-velocity-app-onboarding:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "onboarding:build:dev"
            },
            "qa": {
              "browserTarget": "onboarding:build:qa"
            },
            "train": {
              "browserTarget": "onboarding:build:train"
            },
            "prod": {
              "browserTarget": "onboarding:build:prod"
            },
            "es5": {
              "browserTarget": "onboarding:build:es5"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nni-velocity-app-onboarding:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "tsConfig": "tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"],
            "maxWarnings": 201
          }
        }
      }
    }
  },
  "defaultProject": "nni-velocity-app-onboarding",
  "cli": {
    "analytics": "eefed3cf-e885-46fe-a9db-5001a361039c",
    "defaultCollection": "@angular-eslint/schematics"
  }
}
